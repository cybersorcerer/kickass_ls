        ],
        "related": ["0xD019", "0xD011", "0xD012"],
        "tips": ["Must enable specific interrupt types", "Combined with global interrupt enable"]
      },
      "0xD01B": {
        "name": "Sprite Priority Register",
        "category": "VIC-II",
        "type": "register",
        "size": 1,
        "description": "Controls sprite display priority (foreground/background)",
        "access": "read/write",
        "bitFields": {
          "0": "Sprite 0 priority (0=foreground, 1=background)",
          "1": "Sprite 1 priority",
          "2": "Sprite 2 priority",
          "3": "Sprite 3 priority",
          "4": "Sprite 4 priority",
          "5": "Sprite 5 priority",
          "6": "Sprite 6 priority",
          "7": "Sprite 7 priority"
        },
        "examples": [
          "lda #%00000000 ; All sprites in foreground",
          "sta $d01b",
          "lda #%11111111 ; All sprites behind screen",
          "sta $d01b"
        ],
        "related": ["0xD015", "0xD01E", "0xD01F"],
        "tips": [
          "0 = sprite drawn in front of screen data",
          "1 = sprite drawn behind foreground pixels",
          "Background color pixels always behind sprite",
          "Affects sprite-background collision detection ($D01F)"
        ]
      },
      "0xD01C": {
        "name": "Sprite Multicolor Mode",
        "category": "VIC-II",
        "type": "register",
        "size": 1,
        "description": "Enables multicolor mode for individual sprites",
        "access": "read/write",
        "bitFields": {
          "0": "Sprite 0 multicolor enable",
          "1": "Sprite 1 multicolor enable",
          "2": "Sprite 2 multicolor enable",
          "3": "Sprite 3 multicolor enable",
          "4": "Sprite 4 multicolor enable",
          "5": "Sprite 5 multicolor enable",
          "6": "Sprite 6 multicolor enable",
          "7": "Sprite 7 multicolor enable"
        },
        "examples": [
          "lda #%00000011 ; Multicolor for sprites 0-1",
          "sta $d01c"
        ],
        "related": ["0xD025", "0xD026", "0xD027"],
        "tips": [
          "Multicolor: 4 colors but HALF horizontal resolution",
          "Colors: $D025, $D026, sprite color ($D027-$D02E), transparent",
          "Pixel pairs: 00=transparent, 01=$D025, 10=sprite color, 11=$D026",
          "Mix hires and multicolor sprites for best results"
        ]
      },
