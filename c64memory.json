      "0xD01E": {
        "name": "Sprite-Sprite Collision",
        "category": "VIC-II",
        "type": "register",
        "size": 1,
        "description": "Sprite-to-sprite collision detection register",
        "access": "read",
        "bitFields": {
          "0": "Sprite 0 collision flag",
          "1": "Sprite 1 collision flag",
          "2": "Sprite 2 collision flag",
          "3": "Sprite 3 collision flag",
          "4": "Sprite 4 collision flag",
          "5": "Sprite 5 collision flag",
          "6": "Sprite 6 collision flag",
          "7": "Sprite 7 collision flag"
        },
        "examples": [
          "lda $d01e    ; Read collision register",
          "and #%00000011 ; Check sprites 0-1 collision"
        ],
        "related": ["0xD01F", "0xD019"],
        "tips": [
          "CLEARED ON READ - save value immediately if needed",
          "Bit set when sprite collides with ANY other sprite",
          "Check collision interrupt flag in $D019 first",
          "Reading clears ALL collision bits at once"
        ]
      },
      "0xD01F": {
        "name": "Sprite-Background Collision",
        "category": "VIC-II",
        "type": "register",
        "size": 1,
        "description": "Sprite-to-background collision detection register",
        "access": "read",
        "bitFields": {
          "0": "Sprite 0 background collision",
          "1": "Sprite 1 background collision",
          "2": "Sprite 2 background collision",
          "3": "Sprite 3 background collision",
          "4": "Sprite 4 background collision",
          "5": "Sprite 5 background collision",
          "6": "Sprite 6 background collision",
          "7": "Sprite 7 background collision"
        },
        "examples": [
          "lda $d01f    ; Read collision register",
          "and #$01     ; Check sprite 0 collision"
        ],
        "related": ["0xD01E", "0xD019"],
        "tips": [
          "CLEARED ON READ - save value immediately if needed",
          "Detects collision with foreground screen data only",
          "Priority bit ($D01B) affects which pixels are checked",
          "Reading clears ALL collision bits at once"
        ]
      },
      "0xD020": {
        "name": "Border Color Register",
        "category": "VIC-II",
        "type": "register",
        "size": 1,
        "description": "Controls the color of the screen border",
        "access": "read/write",
        "bitFields": {
          "0-3": "Color value (0-15)"
        },
        "values": {
          "0x00": "Black", "0x01": "White", "0x02": "Red", "0x03": "Cyan",
          "0x04": "Purple", "0x05": "Green", "0x06": "Blue", "0x07": "Yellow",
          "0x08": "Orange", "0x09": "Brown", "0x0A": "Light Red", "0x0B": "Dark Gray",
          "0x0C": "Gray", "0x0D": "Light Green", "0x0E": "Light Blue", "0x0F": "Light Gray"
        },
        "examples": [
